{
    "name": "ggggino/recenty-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to facilitate the 'recently use of something'",
    "homepage": "https://contao.org",
    "license": "MIT",
    "authors": [
        {
            "name": "David Ginanni",
            "homepage": "https://github.com/ggggino"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/http-kernel": "^4.4 || ^5.1",
        "doctrine/doctrine-bundle": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/phpunit-bridge": "^4.4 || ^5.1",
        "psr/cache": "^1.0",
        "psr/simple-cache": "^1.0"
    },
    "suggest": {
        "psr/cache": "To be used with GGGGino\\RecentyBundle\\Storage\\StorageCache",
        "psr/simple-cache": "To be used with GGGGino\\RecentyBundle\\Storage\\StorageCache"
    },
    "autoload": {
        "psr-4": {
            "GGGGino\\RecentyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GGGGino\\RecentyBundle\\Tests\\": "tests"
        }
    }
}
